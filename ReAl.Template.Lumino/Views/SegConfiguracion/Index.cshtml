@model IEnumerable<ReAl.Template.Lumino.Models.SegConfiguracion>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Configuracion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Funcion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Parametrosentrada)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Parametrossalida)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Descripcion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Apiestado)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Apitransaccion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Usucre)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Feccre)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Usumod)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Fecmod)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.IdstaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Configuracion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Funcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Parametrosentrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Parametrossalida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apiestado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apitransaccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usucre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Feccre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usumod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecmod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdstaNavigation.Alias)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idscf">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idscf">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idscf">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
